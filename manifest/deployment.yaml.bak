apiVersion: apps/v1
kind: Deployment
metadata:
  name: react-dep
  labels:
    app: react-dep
spec:
  replicas: 1
  selector:
    matchLabels:
      app: react-dep
  template:
    metadata:
      labels:
        app: react-dep
    spec:
      containers:
      - name: react-dep
        image: farhat18/react-dep:39
        ports:
        - containerPort: 3000
---
apiVersion: v1
kind: Service
metadata:
  name: react-dep
  labels:
    app: react-dep
spec:
  type: NodePort
  ports:
  - nodePort: 30001
    port: 80
    targetPort: 3000
  selector:
    app: react-dep
